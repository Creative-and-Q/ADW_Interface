meta {
  name: Get All Module Processes
  type: http
  seq: 1
}

get {
  url: http://localhost:3035/module-processes
  body: none
  auth: none
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should return success", function() {
    expect(res.body.success).to.equal(true);
  });

  test("should return array of modules", function() {
    expect(res.body.data).to.be.an('array');
    expect(res.body.data.length).to.be.greaterThan(0);
  });

  test("each module should have required fields", function() {
    const module = res.body.data[0];
    expect(module).to.have.property('name');
    expect(module).to.have.property('status');
    expect(module).to.have.property('port');
    expect(module).to.have.property('restartCount');
  });
}
