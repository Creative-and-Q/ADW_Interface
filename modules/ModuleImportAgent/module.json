{
  "name": "ModuleImportAgent",
  "version": "1.0.0",
  "description": "AI-powered agent that automatically generates module.json configurations for newly imported modules with environment variable detection",
  "category": "Agents",
  "project": "AIDeveloper",
  "tags": [
    "ai",
    "module-import",
    "automation",
    "agent",
    "configuration"
  ],
  "scripts": {
    "install": "npm install",
    "build": "npm run build",
    "typecheck": "npm run typecheck"
  },
  "envVars": [
    {
      "key": "OPENROUTER_API_KEY",
      "description": "OpenRouter API key for AI-powered module analysis",
      "required": true,
      "type": "string",
      "secret": true
    },
    {
      "key": "OPENROUTER_MODEL",
      "description": "OpenRouter AI model to use for module analysis",
      "required": false,
      "defaultValue": "anthropic/claude-3.5-sonnet",
      "type": "string",
      "secret": false
    },
    {
      "key": "AIDEVELOPER_API_URL",
      "description": "AIDeveloper API URL for triggering workflows",
      "required": false,
      "defaultValue": "http://localhost:3000",
      "type": "string",
      "secret": false
    }
  ]
}
